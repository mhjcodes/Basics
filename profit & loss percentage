#include<stdio.h>
int main()
{
    float a,b,c;
    float d;
    printf("enter:cost|selling price of product");
    scanf("%f|%f",&a,&b);
    d= (b-a)/a;
    if (d>0)
    printf("profit percentage is %f",d);
    else
    {
        d*=-1;
        printf("loss percentage is %f",d);
    }
    return 0;
}
