Lcm of two no


#include<stdio.h>
int main()
{
    int a,b,c;
    printf("Enter two numbers");
    scanf("%d %d",&a,&b);
    c= (a>b)?a:b;
    if (c%a==0 && c%b==0)
        printf("LCM of two nos is %d",c);
    else
    {
        c= a*b;
        printf("LCM of two nos is %d",c);
    }
    return 0;
}


reverse order of no



#include<stdio.h>
int main()
{
    int a,b,c=0;
    printf("Enter a no");
    scanf("%d",&a);
    while(1)
    {
        b=a%10;
        c=c*10+b;
        a=a/10;
        if(a==0)
        break;
    }
    printf("%d",c);
    return 0;
}


HCF of two nos


#include<stdio.h>
int main()
{
    int a,b,c;
    printf("Enter two numbers");
    scanf("%d %d",&a,&b);
    c=a>b?a:b;
    if(c%a==0 && c%b==0)
    printf("%d",a>b?b:a);
    else
    printf("1");
    return 0;
}
