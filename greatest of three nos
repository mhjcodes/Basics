#include <stdio.h>
int main()
{ 
    int a,b,c,d;
    printf("enter 1st no ,2nd no ,3rd no");
    scanf("%d,%d,%d",&a,&b,&c);
    d= a>c && a>b;
    if(d)
    printf("%d",&a);
    else
    {
        d= b>a && b>c;
        if(d)
        printf("%d",b);
        else
        printf("%d",c);
    }
    return 0;
}


method 2

#include<stdio.h>
int main()
{
    int a,b,c;
    printf("Enter three numbers");
    scanf("%d %d %d",&a,&b,&c);
    if(a>b && a>c)
    printf("%d",a);
    else
    {
        if(b>c)
        printf("%d",b);
        else
        printf("%d",c);
    }
    return 0;
}


method 2 (opt)


#include<stdio.h>
int main()
{
    int a,b,c;
    printf("Enter three numbers");
    scanf("%d %d %d",&a,&b,&c);
    if(a>b)
    {
        if(a>c)
        printf("%d",a);
        else
        printf("%d",c);
    }
    else
    {
        if(b>c)
         printf("%d",b);
        else
         printf("%d",c);
    }
    return 0;
}
