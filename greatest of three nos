#include <stdio.h>
int main()
{ 
    int a,b,c,d;
    printf("enter 1st no ,2nd no ,3rd no");
    scanf("%d,%d,%d",&a,&b,&c);
    d= a>c && a>b;
    if(d)
    printf("%d",&a);
    else
    {
        d= b>a && b>c;
        if(d)
        printf("%d",b);
        else
        printf("%d",c);
    }
    return 0;
}
