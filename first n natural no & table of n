first N natural no



#include<stdio.h>
int main()
{
   int a=1,n;
   printf ("Enter no ");
   scanf("%d",&n);

    while (a<=n)
    {
        printf("%d ",a);
        a++;
    }
    return 0;
}


table of N

#include<stdio.h>
int main()
{
    int a,b,n;
    printf("Enter a no");
    scanf("%d",&n);
    for(a=1;a<=12;a++)
    {
       b= n*a;
       printf("%d*%d=%d\n",n,a,b);
    }
    return 0;

}
