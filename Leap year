#include<stdio.h>
int main()
{
    int a;
    printf("Enter a year");
    scanf("%d",&a);
    if(a%100)
    {
        // not a century year
        if(a%4)
        printf("Not a leap year");
        else
        printf("Leap year");
    }
    else
    {
        // century year
        if(a%400)
        printf("Not a leap year");
        else
        printf("Leap year");
    }
    return 0;
}
