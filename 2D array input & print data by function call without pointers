#include<stdio.h>
void f1(int b[][4], int row );
void f2(int c[][4], int row);
int main()
{
    int a[3][4];
    f2(a,3);
    f1(a,3);
    return 0;
}
void f1(int b[][4], int row )                               /* u cant kali chod column */
{
    int i,j;
    for(i=0;i<row;i++)
    {
        for(j=0;j<4;j++)
         printf("%d ",b[i][j]);
         printf("\n");
    }
}
void f2(int c[][4], int row)
{
    int i,j;
    for(i=0;i<row;i++)
    {
        printf("Enter four numbers");
        for(j=0;j<4;j++)
        scanf("%d",&c[i][j]);
    }
}
