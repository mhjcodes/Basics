#include<stdio.h>
int main()
{
    int a=0,b=1,c,i,j;
    printf("Enter no");
    scanf("%d",&j);
    if(j)
    {
    for(i=1;1;i++)
    {
        c=a+b;
        a=b;
        b=c;
        if(a==j)
        {
            printf("Yes");
            break;
        }
        if(a>j)
        {
            printf("No");
            break;
        }
    }
    }
    else
    printf("Yes");
    return 0;
}
