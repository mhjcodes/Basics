prime no print betw two nos



#include<stdio.h>
int main()
{
    int a,b,c,e,f;
    printf("Enter two nos");
    scanf("%d %d",&a,&f);
    for(b=a+1;b<f;b++)
    {
        e=0;
        for(c=1;c<=b;c++)
        {
            if(b%c==0)
            e++;
            
        }
        if(e==2)
        printf("%d ",b);
    }
    return 0;
}


prime no test



#include<stdio.h>
int main()
{
    int a,i;
    printf("Enter a no");
    scanf("%d",&a);
    for(i=2;i<=a-1;i++)
    {
         if(a%i==0)
         {
            printf("Not a Prime no");
            break;
         }

    }
    if(i==a)
    printf("Prime no");
    return 0;
}
